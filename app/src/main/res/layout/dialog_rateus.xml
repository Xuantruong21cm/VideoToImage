<?xml version="1.0" encoding="utf-8"?>
<FrameLayout  xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:id="@+id/dialog_rateStar"
        android:layout_gravity="center"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_200sdp">
        <LinearLayout
            android:orientation="vertical"
            android:weightSum="2"
            android:layout_gravity="center"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_100sdp">
            <RatingBar
                android:layout_gravity="center"
                android:id="@+id/ratingbar_star"
                android:layout_width="@dimen/_160sdp"
                android:layout_height="0dp"
                android:layout_weight="1"/>
            <ImageView
                android:background="@drawable/custom_clickview"
                android:layout_gravity="center"
                android:id="@+id/img_rateStar"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:src="@drawable/rate"/>
        </LinearLayout>
    </FrameLayout>

</FrameLayout>